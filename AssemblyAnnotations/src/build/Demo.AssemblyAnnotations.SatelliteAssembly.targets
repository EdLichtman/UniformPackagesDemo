<Project>
    <PropertyGroup Condition=" '$(DemoAssemblyType)' == 'SatelliteAssembly' ">
        <IsPublishable>false</IsPublishable>
        <IsPackable>false</IsPackable>
        <OutputType>Library</OutputType>
    </PropertyGroup>

    <PropertyGroup Condition="  '$(DemoAssemblyType)' == 'SatelliteAssembly' and '$(Configuration)' != 'Release' ">
        <DebugSymbols>true</DebugSymbols>
        <DebugType>Full</DebugType>
    </PropertyGroup>
</Project>